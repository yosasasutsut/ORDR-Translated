)call PreloadEnd(3.0)endifendfunctionfunction UnitAngle takes unit u returns realreturn bj_RADTODEG*Atan2(GetUnitY(u)-GetUnitY(udg_unit_laily),GetUnitX(u)-GetUnitX(udg_unit_laily))endfunctionfunction ScreenFilter takes string str,blendmode bm,integer r1,integer g1,integer b1,integer a1,integer r2,integer g2,integer b2,integer a2,real dur returns nothingcall SetCineFilterTexture(str)call SetCineFilterBlendMode(bm)call SetCineFilterStartUV(0,0,1,1)call SetCineFilterEndUV(0,0,1,1)call SetCineFilterStartColor(r1,g1,b1,a1)call SetCineFilterEndColor(r2,g2,b2,a2)call SetCineFilterDuration(dur)call DisplayCineFilter(true)endfunctionfunction CinematicModeExDiet takes boolean cineMode,force forForce,real interfaceFadeTime returns nothingif not bj_gameStarted thenset interfaceFadeTime=0endifif cineMode thenif IsPlayerInForce(GetLocalPlayer(),forForce)thencall ClearTextMessages()call ShowInterface(false,interfaceFadeTime)call EnableUserControl(false)call EnableOcclusion(false)endifelseif IsPlayerInForce(GetLocalPlayer(),forForce)thencall ShowInterface(true,interfaceFadeTime)call EnableUserControl(true)call EnableOcclusion(true)endifendifendfunctionfunction KBHFunc takes nothing returns nothinglocal timer t=GetExpiredTimer()local unit target=LoadUnitHandle(bj_lastCreatedHashtable,GetHandleId(t),StringHash("target"))local effect elocal string S=LoadStr(bj_lastCreatedHashtable,GetHandleId(t),StringHash("S"))local real R=LoadReal(bj_lastCreatedHashtable,GetHandleId(t),StringHash("d"))local boolean f=LoadBoolean(bj_lastCreatedHashtable,GetHandleId(t),StringHash("f"))if LoadReal(bj_lastCreatedHashtable,GetHandleId(t),StringHash("in"))<LoadReal(bj_lastCreatedHashtable,GetHandleId(t),StringHash("out"))thenif S!=null thenset e=AddSpecialEffectTarget(S,target,"origin")call DestroyEffect(e)endifif f==false thenif IsTerrainPathable(GetUnitX(target)+CosBJ(LoadReal(bj_lastCreatedHashtable,GetHandleId(t),StringHash("ang")))*R,GetUnitY(target)+S